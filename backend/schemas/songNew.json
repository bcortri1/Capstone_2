{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "title",
        "data",
        "author"
    ],
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "minLength":3,
            "default": "New Song",
            "examples": [
                "Song1"
            ]
        },
        "data": {
            "title": "The data Schema",
            "type": "array",
            "default": [],
            "items": {},
            "examples": [
                []
            ]
        },
        "author": {
            "title": "The author Schema",
            "type": "string",
            "minLength":3,
            "default": "",
            "examples": [
                "bill"
            ]
        }
    },
    "examples": [{
        "title": "Song1",
        "data": [],
        "author": "bill"
    }]
}