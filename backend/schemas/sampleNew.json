{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "name",
        "sound",
        "user"
    ],
    "properties": {
        "name": {
            "title": "The name Schema",
            "type": "string",
            "default": "New Sample",
            "minLength":3,
            "examples": [
                "Sample1"
            ]
        },
        "sound": {
            "title": "The sound Schema",
            "type": "array",
            "default": [],
            "items": {},
            "examples": [
                []
            ]
        },
        "user": {
            "title": "The user Schema",
            "type": "string",
            "default": "",
            "examples": [
                "bill"
            ]
        }
    },
    "examples": [{
        "name": "Sample1",
        "sound": [],
        "user": "bill"
    }]
}